
<!--  Login Request --> 


{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
      "data": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string"
          },
          "client_id": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        },
        "required": [
          "username",
          "client_id",
          "password"
        ]
      }
    },
    "required": [
      "data"
    ]
  }


      <!--  Login Response --> 

{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
      "data": {
        "type": "object",
        "properties": {
          "result": {
            "type": "object",
            "properties": {
              "access_token": {
                "type": "string"
              },
              "expires_after": {
                "type": "string"
              }
            },
            "required": [
              "access_token",
              "expires_after"
            ]
          }
        },
        "required": [
          "result"
        ]
      },
      "status": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string"
          }
        },
        "required": [
          "type"
        ]
      }
    },
    "required": [
      "data",
      "status"
    ]
  }




