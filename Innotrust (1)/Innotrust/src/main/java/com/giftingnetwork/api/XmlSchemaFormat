<xs:simpleType name="WhereFieldNameEnum"> 
	<xs:restriction base="xs:string">
		<xs:enumeration value="GroupID"/>
        <xs:enumeration value="company_name"/>
 	</xs:restriction>
</xs:simpleType>

<xs:element name="SqlCommand">
 	<xs:complexType>
		<xs:sequence>
        		<xs:element name="Select" type="SelectClauseType" />
				<xs:element name="Where" type="WhereClauseType" />
				<xs:element name="Paging" type="PagingType" />
        </xs:sequence>
 	</xs:complexType>
</xs:element>

	<xs:simpleType name="OperatorEnum"> 
		<xs:restriction base="xs:string">
			<xs:enumeration value="="/>
			<xs:enumeration value="!="/> 
			<xs:enumeration value="&lt;"/>
			 <xs:enumeration value="&lt;="/> 
			<xs:enumeration value="&gt;"/>
			 <xs:enumeration value="&gt;="/> 
			<xs:enumeration value="null"/>
			 <xs:enumeration value="notnull"/> 
			<xs:enumeration value="like"/> 
			<xs:enumeration value="notlike"/> 
			<xs:enumeration value="contain"/>
			 <xs:enumeration value="notcontain"/>
			 <xs:enumeration value="in"/>
			<xs:enumeration value="notin"/>
      		  </xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="CompareOptionsEnum">
		 <xs:restriction base="xs:string">
			<xs:enumeration value="TreatNullAsEmpty"/>
			<xs:enumeration value="TreatEmptyAsNull"/> 
		</xs:restriction>
	</xs:simpleType>


<xs:complexType name="SelectClauseType"> 
	<xs:sequence>
		<xs:element name="Field" maxOccurs="unbounded" minOccurs="0">
		 	<xs:complexType>
                <xs:simpleContent>
                    <xs:extension base="xs:string">
						<xs:attribute type="SelectFieldNameEnum" name="Name"/>
                      </xs:extension>
               	</xs:simpleContent>
        	</xs:complexType>
		</xs:element>
	</xs:sequence>
</xs:complexType>

     <xs:complexType name="WhereClauseType"> 
		<xs:sequence>
			<xs:element name="Field" maxOccurs="unbounded" minOccurs="0"> 
				<xs:complexType>
                			<xs:simpleContent>
                         			<xs:extension base="xs:string">
							<xs:attribute type="WhereFieldNameEnum" name="Name" use="required"/>
							<xs:attribute type="OperatorEnum" name="Operator" use="required"/>
							<xs:attribute type="CompareOptionsEnum" name="CompareOptions" use="optional"/>
                         			</xs:extension>
                			</xs:simpleContent>
       				 </xs:complexType>
			</xs:element>

			<xs:element name="Param" maxOccurs="unbounded" minOccurs="0"> 
				<xs:complexType>
                			<xs:simpleContent>
                        			 <xs:extension base="xs:string">
							<xs:attribute type="xs:string" name="Name" use="required"/>
						</xs:extension>
               				 </xs:simpleContent>
       				 </xs:complexType>
			</xs:element>
		</xs:sequence>
        </xs:complexType>

	<xs:complexType name="PagingType"> 
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute type="xs:short" name="_pageNumber" use="required"/> 
				<xs:attribute type="xs:short" name="_pageSize" use="required"/> 
				<xs:attribute type="xs:string" name="_orderBy" use="required"/>
                         </xs:extension>
                </xs:simpleContent>
        </xs:complexType>

</xs:schema>